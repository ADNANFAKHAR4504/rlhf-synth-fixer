{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Three-Tier VPC Architecture for Environment Migration - CloudFormation Template",
  "Parameters": {
    "EnvironmentSuffix": {
      "Type": "String",
      "Default": "dev",
      "Description": "Environment suffix for resource naming (e.g., dev, staging, prod)",
      "AllowedPattern": "^[a-zA-Z0-9]+$",
      "ConstraintDescription": "Must contain only alphanumeric characters"
    },
    "ProjectName": {
      "Type": "String",
      "Default": "migration",
      "Description": "Project name for resource tagging"
    },
    "EnvironmentType": {
      "Type": "String",
      "Default": "production",
      "AllowedValues": ["development", "staging", "production"],
      "Description": "Environment type for resource tagging"
    }
  },
  "Resources": {
    "VPC": {
      "Type": "AWS::EC2::VPC",
      "Properties": {
        "CidrBlock": "10.0.0.0/16",
        "EnableDnsHostnames": true,
        "EnableDnsSupport": true,
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-vpc-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "InternetGateway": {
      "Type": "AWS::EC2::InternetGateway",
      "Properties": {
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-igw-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "VPCGatewayAttachment": {
      "Type": "AWS::EC2::VPCGatewayAttachment",
      "Properties": {
        "VpcId": { "Ref": "VPC" },
        "InternetGatewayId": { "Ref": "InternetGateway" }
      }
    },
    "PublicSubnetAZ1": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": { "Ref": "VPC" },
        "CidrBlock": "10.0.1.0/24",
        "AvailabilityZone": {
          "Fn::Select": [0, { "Fn::GetAZs": "" }]
        },
        "MapPublicIpOnLaunch": true,
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-public-subnet-az1-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          },
          {
            "Key": "Tier",
            "Value": "web"
          }
        ]
      }
    },
    "PublicSubnetAZ2": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": { "Ref": "VPC" },
        "CidrBlock": "10.0.2.0/24",
        "AvailabilityZone": {
          "Fn::Select": [1, { "Fn::GetAZs": "" }]
        },
        "MapPublicIpOnLaunch": true,
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-public-subnet-az2-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          },
          {
            "Key": "Tier",
            "Value": "web"
          }
        ]
      }
    },
    "PrivateSubnetAZ1": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": { "Ref": "VPC" },
        "CidrBlock": "10.0.11.0/24",
        "AvailabilityZone": {
          "Fn::Select": [0, { "Fn::GetAZs": "" }]
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-private-subnet-az1-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          },
          {
            "Key": "Tier",
            "Value": "application"
          }
        ]
      }
    },
    "PrivateSubnetAZ2": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": { "Ref": "VPC" },
        "CidrBlock": "10.0.12.0/24",
        "AvailabilityZone": {
          "Fn::Select": [1, { "Fn::GetAZs": "" }]
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-private-subnet-az2-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          },
          {
            "Key": "Tier",
            "Value": "application"
          }
        ]
      }
    },
    "IsolatedSubnetAZ1": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": { "Ref": "VPC" },
        "CidrBlock": "10.0.21.0/24",
        "AvailabilityZone": {
          "Fn::Select": [0, { "Fn::GetAZs": "" }]
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-isolated-subnet-az1-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          },
          {
            "Key": "Tier",
            "Value": "database"
          }
        ]
      }
    },
    "IsolatedSubnetAZ2": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": { "Ref": "VPC" },
        "CidrBlock": "10.0.22.0/24",
        "AvailabilityZone": {
          "Fn::Select": [1, { "Fn::GetAZs": "" }]
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-isolated-subnet-az2-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          },
          {
            "Key": "Tier",
            "Value": "database"
          }
        ]
      }
    },
    "EIPAZ1": {
      "Type": "AWS::EC2::EIP",
      "DependsOn": "VPCGatewayAttachment",
      "Properties": {
        "Domain": "vpc",
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-eip-az1-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "EIPAZ2": {
      "Type": "AWS::EC2::EIP",
      "DependsOn": "VPCGatewayAttachment",
      "Properties": {
        "Domain": "vpc",
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-eip-az2-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "NATGatewayAZ1": {
      "Type": "AWS::EC2::NatGateway",
      "Properties": {
        "AllocationId": { "Fn::GetAtt": ["EIPAZ1", "AllocationId"] },
        "SubnetId": { "Ref": "PublicSubnetAZ1" },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-nat-az1-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "NATGatewayAZ2": {
      "Type": "AWS::EC2::NatGateway",
      "Properties": {
        "AllocationId": { "Fn::GetAtt": ["EIPAZ2", "AllocationId"] },
        "SubnetId": { "Ref": "PublicSubnetAZ2" },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-nat-az2-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "PublicRouteTable": {
      "Type": "AWS::EC2::RouteTable",
      "Properties": {
        "VpcId": { "Ref": "VPC" },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-public-rt-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "PublicRoute": {
      "Type": "AWS::EC2::Route",
      "DependsOn": "VPCGatewayAttachment",
      "Properties": {
        "RouteTableId": { "Ref": "PublicRouteTable" },
        "DestinationCidrBlock": "0.0.0.0/0",
        "GatewayId": { "Ref": "InternetGateway" }
      }
    },
    "PublicSubnetRouteTableAssociationAZ1": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": { "Ref": "PublicSubnetAZ1" },
        "RouteTableId": { "Ref": "PublicRouteTable" }
      }
    },
    "PublicSubnetRouteTableAssociationAZ2": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": { "Ref": "PublicSubnetAZ2" },
        "RouteTableId": { "Ref": "PublicRouteTable" }
      }
    },
    "PrivateRouteTableAZ1": {
      "Type": "AWS::EC2::RouteTable",
      "Properties": {
        "VpcId": { "Ref": "VPC" },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-private-rt-az1-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "PrivateRouteAZ1": {
      "Type": "AWS::EC2::Route",
      "Properties": {
        "RouteTableId": { "Ref": "PrivateRouteTableAZ1" },
        "DestinationCidrBlock": "0.0.0.0/0",
        "NatGatewayId": { "Ref": "NATGatewayAZ1" }
      }
    },
    "PrivateSubnetRouteTableAssociationAZ1": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": { "Ref": "PrivateSubnetAZ1" },
        "RouteTableId": { "Ref": "PrivateRouteTableAZ1" }
      }
    },
    "PrivateRouteTableAZ2": {
      "Type": "AWS::EC2::RouteTable",
      "Properties": {
        "VpcId": { "Ref": "VPC" },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-private-rt-az2-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "PrivateRouteAZ2": {
      "Type": "AWS::EC2::Route",
      "Properties": {
        "RouteTableId": { "Ref": "PrivateRouteTableAZ2" },
        "DestinationCidrBlock": "0.0.0.0/0",
        "NatGatewayId": { "Ref": "NATGatewayAZ2" }
      }
    },
    "PrivateSubnetRouteTableAssociationAZ2": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": { "Ref": "PrivateSubnetAZ2" },
        "RouteTableId": { "Ref": "PrivateRouteTableAZ2" }
      }
    },
    "IsolatedRouteTableAZ1": {
      "Type": "AWS::EC2::RouteTable",
      "Properties": {
        "VpcId": { "Ref": "VPC" },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-isolated-rt-az1-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "IsolatedSubnetRouteTableAssociationAZ1": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": { "Ref": "IsolatedSubnetAZ1" },
        "RouteTableId": { "Ref": "IsolatedRouteTableAZ1" }
      }
    },
    "IsolatedRouteTableAZ2": {
      "Type": "AWS::EC2::RouteTable",
      "Properties": {
        "VpcId": { "Ref": "VPC" },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-isolated-rt-az2-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "IsolatedSubnetRouteTableAssociationAZ2": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": { "Ref": "IsolatedSubnetAZ2" },
        "RouteTableId": { "Ref": "IsolatedRouteTableAZ2" }
      }
    },
    "WebServerSecurityGroup": {
      "Type": "AWS::EC2::SecurityGroup",
      "Properties": {
        "GroupName": {
          "Fn::Sub": "${ProjectName}-web-sg-${EnvironmentSuffix}"
        },
        "GroupDescription": "Security group for web servers allowing HTTP and HTTPS",
        "VpcId": { "Ref": "VPC" },
        "SecurityGroupIngress": [
          {
            "IpProtocol": "tcp",
            "FromPort": 80,
            "ToPort": 80,
            "CidrIp": "0.0.0.0/0",
            "Description": "Allow HTTP from anywhere"
          },
          {
            "IpProtocol": "tcp",
            "FromPort": 443,
            "ToPort": 443,
            "CidrIp": "0.0.0.0/0",
            "Description": "Allow HTTPS from anywhere"
          }
        ],
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-web-sg-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "AppServerSecurityGroup": {
      "Type": "AWS::EC2::SecurityGroup",
      "Properties": {
        "GroupName": {
          "Fn::Sub": "${ProjectName}-app-sg-${EnvironmentSuffix}"
        },
        "GroupDescription": "Security group for application servers allowing traffic from web tier only",
        "VpcId": { "Ref": "VPC" },
        "SecurityGroupIngress": [
          {
            "IpProtocol": "tcp",
            "FromPort": 8080,
            "ToPort": 8080,
            "SourceSecurityGroupId": { "Ref": "WebServerSecurityGroup" },
            "Description": "Allow port 8080 from web servers only"
          }
        ],
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-app-sg-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "DatabaseSecurityGroup": {
      "Type": "AWS::EC2::SecurityGroup",
      "Properties": {
        "GroupName": {
          "Fn::Sub": "${ProjectName}-db-sg-${EnvironmentSuffix}"
        },
        "GroupDescription": "Security group for database servers allowing traffic from app tier only",
        "VpcId": { "Ref": "VPC" },
        "SecurityGroupIngress": [
          {
            "IpProtocol": "tcp",
            "FromPort": 3306,
            "ToPort": 3306,
            "SourceSecurityGroupId": { "Ref": "AppServerSecurityGroup" },
            "Description": "Allow MySQL from app servers only"
          }
        ],
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-db-sg-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "PublicNetworkAcl": {
      "Type": "AWS::EC2::NetworkAcl",
      "Properties": {
        "VpcId": { "Ref": "VPC" },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-public-nacl-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "PublicNetworkAclIngressHTTP": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "NetworkAclId": { "Ref": "PublicNetworkAcl" },
        "RuleNumber": 100,
        "Protocol": 6,
        "RuleAction": "allow",
        "CidrBlock": "0.0.0.0/0",
        "PortRange": {
          "From": 80,
          "To": 80
        }
      }
    },
    "PublicNetworkAclIngressHTTPS": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "NetworkAclId": { "Ref": "PublicNetworkAcl" },
        "RuleNumber": 110,
        "Protocol": 6,
        "RuleAction": "allow",
        "CidrBlock": "0.0.0.0/0",
        "PortRange": {
          "From": 443,
          "To": 443
        }
      }
    },
    "PublicNetworkAclIngressEphemeral": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "NetworkAclId": { "Ref": "PublicNetworkAcl" },
        "RuleNumber": 120,
        "Protocol": 6,
        "RuleAction": "allow",
        "CidrBlock": "0.0.0.0/0",
        "PortRange": {
          "From": 1024,
          "To": 65535
        }
      }
    },
    "PublicNetworkAclEgressAll": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "NetworkAclId": { "Ref": "PublicNetworkAcl" },
        "RuleNumber": 100,
        "Protocol": -1,
        "Egress": true,
        "RuleAction": "allow",
        "CidrBlock": "0.0.0.0/0"
      }
    },
    "PublicSubnetNetworkAclAssociationAZ1": {
      "Type": "AWS::EC2::SubnetNetworkAclAssociation",
      "Properties": {
        "SubnetId": { "Ref": "PublicSubnetAZ1" },
        "NetworkAclId": { "Ref": "PublicNetworkAcl" }
      }
    },
    "PublicSubnetNetworkAclAssociationAZ2": {
      "Type": "AWS::EC2::SubnetNetworkAclAssociation",
      "Properties": {
        "SubnetId": { "Ref": "PublicSubnetAZ2" },
        "NetworkAclId": { "Ref": "PublicNetworkAcl" }
      }
    },
    "PrivateNetworkAcl": {
      "Type": "AWS::EC2::NetworkAcl",
      "Properties": {
        "VpcId": { "Ref": "VPC" },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-private-nacl-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "PrivateNetworkAclIngressApp": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "NetworkAclId": { "Ref": "PrivateNetworkAcl" },
        "RuleNumber": 100,
        "Protocol": 6,
        "RuleAction": "allow",
        "CidrBlock": "10.0.0.0/16",
        "PortRange": {
          "From": 8080,
          "To": 8080
        }
      }
    },
    "PrivateNetworkAclIngressEphemeral": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "NetworkAclId": { "Ref": "PrivateNetworkAcl" },
        "RuleNumber": 110,
        "Protocol": 6,
        "RuleAction": "allow",
        "CidrBlock": "0.0.0.0/0",
        "PortRange": {
          "From": 1024,
          "To": 65535
        }
      }
    },
    "PrivateNetworkAclEgressAll": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "NetworkAclId": { "Ref": "PrivateNetworkAcl" },
        "RuleNumber": 100,
        "Protocol": -1,
        "Egress": true,
        "RuleAction": "allow",
        "CidrBlock": "0.0.0.0/0"
      }
    },
    "PrivateSubnetNetworkAclAssociationAZ1": {
      "Type": "AWS::EC2::SubnetNetworkAclAssociation",
      "Properties": {
        "SubnetId": { "Ref": "PrivateSubnetAZ1" },
        "NetworkAclId": { "Ref": "PrivateNetworkAcl" }
      }
    },
    "PrivateSubnetNetworkAclAssociationAZ2": {
      "Type": "AWS::EC2::SubnetNetworkAclAssociation",
      "Properties": {
        "SubnetId": { "Ref": "PrivateSubnetAZ2" },
        "NetworkAclId": { "Ref": "PrivateNetworkAcl" }
      }
    },
    "IsolatedNetworkAcl": {
      "Type": "AWS::EC2::NetworkAcl",
      "Properties": {
        "VpcId": { "Ref": "VPC" },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ProjectName}-isolated-nacl-${EnvironmentSuffix}"
            }
          },
          {
            "Key": "Environment",
            "Value": { "Ref": "EnvironmentType" }
          },
          {
            "Key": "MigrationPhase",
            "Value": "network-setup"
          }
        ]
      }
    },
    "IsolatedNetworkAclIngressDB": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "NetworkAclId": { "Ref": "IsolatedNetworkAcl" },
        "RuleNumber": 100,
        "Protocol": 6,
        "RuleAction": "allow",
        "CidrBlock": "10.0.0.0/16",
        "PortRange": {
          "From": 3306,
          "To": 3306
        }
      }
    },
    "IsolatedNetworkAclIngressEphemeral": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "NetworkAclId": { "Ref": "IsolatedNetworkAcl" },
        "RuleNumber": 110,
        "Protocol": 6,
        "RuleAction": "allow",
        "CidrBlock": "10.0.0.0/16",
        "PortRange": {
          "From": 1024,
          "To": 65535
        }
      }
    },
    "IsolatedNetworkAclEgressAll": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "NetworkAclId": { "Ref": "IsolatedNetworkAcl" },
        "RuleNumber": 100,
        "Protocol": -1,
