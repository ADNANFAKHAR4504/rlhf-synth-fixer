{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "bin/**/*",
    "lib/**/*",
    "index.ts",
    "cli/**/*",
    "test/**/*",
    "tests/**/*"
  ],
  "exclude": [
    "node_modules",
    "cdk.out",
    "dist",
    "coverage",
    "worktree",
    "archive",
    "templates",
    "subcategory-references"
  ]
}