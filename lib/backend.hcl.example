# Example S3 backend configuration for Terraform state
# Copy this file to backend.hcl and fill in your real values.

bucket         = "YOUR_TF_STATE_BUCKET"
key            = "iac-test-automations/lib/terraform.tfstate"
region         = "us-west-2"
# dynamodb_table = "YOUR_TF_STATE_LOCKS_TABLE"  # Optional but recommended for state locking
encrypt        = true