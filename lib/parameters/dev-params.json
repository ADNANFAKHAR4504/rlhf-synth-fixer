{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Parameters": {
    "EnvironmentName": {
      "Type": "String",
      "Default": "dev"
    },
    "SnsEmail": {
      "Type": "String",
      "Default": "dev-team@example.com"
    },
    "VpcCidr": {
      "Type": "String",
      "Default": "10.0.0.0/16"
    },
    "AvailabilityZone1": {
      "Type": "AWS::EC2::AvailabilityZone::Name",
      "Default": "us-east-1a"
    },
    "AvailabilityZone2": {
      "Type": "AWS::EC2::AvailabilityZone::Name",
      "Default": "us-east-1b"
    },
    "AvailabilityZone3": {
      "Type": "AWS::EC2::AvailabilityZone::Name",
      "Default": "us-east-1c"
    },
    "NetworkStackTemplateUrl": {
      "Type": "String",
      "Default": "https://s3.amazonaws.com/your-bucket/nested/NetworkStack.json"
    },
    "ComputeStackTemplateUrl": {
      "Type": "String",
      "Default": "https://s3.amazonaws.com/your-bucket/nested/ComputeStack.json"
    },
    "StorageStackTemplateUrl": {
      "Type": "String",
      "Default": "https://s3.amazonaws.com/your-bucket/nested/StorageStack.json"
    },
    "MonitoringStackTemplateUrl": {
      "Type": "String",
      "Default": "https://s3.amazonaws.com/your-bucket/nested/MonitoringStack.json"
    }
  },
  "Resources": {},
  "Outputs": {
    "EnvironmentName": {
      "Value": {
        "Ref": "EnvironmentName"
      }
    },
    "SnsEmail": {
      "Value": {
        "Ref": "SnsEmail"
      }
    },
    "VpcCidr": {
      "Value": {
        "Ref": "VpcCidr"
      }
    },
    "AvailabilityZone1": {
      "Value": {
        "Ref": "AvailabilityZone1"
      }
    },
    "AvailabilityZone2": {
      "Value": {
        "Ref": "AvailabilityZone2"
      }
    },
    "AvailabilityZone3": {
      "Value": {
        "Ref": "AvailabilityZone3"
      }
    },
    "NetworkStackTemplateUrl": {
      "Value": {
        "Ref": "NetworkStackTemplateUrl"
      }
    },
    "ComputeStackTemplateUrl": {
      "Value": {
        "Ref": "ComputeStackTemplateUrl"
      }
    },
    "StorageStackTemplateUrl": {
      "Value": {
        "Ref": "StorageStackTemplateUrl"
      }
    },
    "MonitoringStackTemplateUrl": {
      "Value": {
        "Ref": "MonitoringStackTemplateUrl"
      }
    }
  }
}