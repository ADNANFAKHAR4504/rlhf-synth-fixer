[run]
source = .
omit =
    */tests/*
    */test/*
    */__pycache__/*
    */site-packages/*
    */virtualenvs/*
    */archive/*
    */templates/*
    */subcategory-references/*
    */.claude/*
    */scripts/*
    */setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
