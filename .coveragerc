[run]
source = lib
omit =
    */tests/*
    */test/*
    */__pycache__/*
    */site-packages/*
    */dist-packages/*
    */lambda/*
    lib/lambda/*

[report]
precision = 2
show_missing = True
skip_covered = False

[html]
directory = coverage/html

[json]
output = coverage/coverage-summary.json
pretty_print = True
