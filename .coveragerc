[run]
source = lib
omit =
    tests/*
    setup.py
    __init__.py
    lib/__init__.py
    lib/tap.py
    lib/lib/*
    lib/tests/*
    lib/cleanup_resources.py
    lib/cleanup_orphaned_resources.sh

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
fail_under = 90
