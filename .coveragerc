[run]
source = lib
omit =
    */test/*
    */tests/*
    */__pycache__/*
    */site-packages/*
    */dist-packages/*
    */.venv/*
    */venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract
precision = 2

[html]
directory = coverage_html_report
