{
  "name": "tap",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": "22.17.0",
    "npm": ">=10.0.0"
  },
  "bin": {
    "tap": "bin/tap.js"
  },
  "scripts": {
    "check-node": "node -e \"const v=process.version; const req='v22.17.0'; if(v!==req){console.error('❌ Node.js version mismatch! Required:',req,'Current:',v); process.exit(1);} console.log('✅ Node.js version is correct:',v);\"",
    "check-python": "python -c \"import sys; req_major=3; req_minor=12; curr_major=sys.version_info.major; curr_minor=sys.version_info.minor; curr_micro=sys.version_info.micro; curr=f'{curr_major}.{curr_minor}.{curr_micro}'; print(f'❌ Python version incompatible! Required: {req_major}.{req_minor}.x, Current: {curr}') or exit(1) if curr_major != req_major or curr_minor != req_minor else print(f'✅ Python version is compatible: {curr}');\"",
    "check-pipenv": "bash -c \"req='2025.0.4'; curr=$(pipenv --version 2>/dev/null | grep -oE '[0-9]+\\.[0-9]+\\.[0-9]+' || echo 'not installed'); if [ \\\"\\$curr\\\" != \\\"\\$req\\\" ]; then echo \\\"❌ Pipenv version mismatch! Required: \\$req, Current: \\$curr\\\"; exit 1; fi; echo \\\"✅ Pipenv version is correct: \\$curr\\\"\"",
    "check-versions": "npm run check-node && npm run check-python && npm run check-pipenv",
    "preinstall": "npm run check-versions",
    "build": "tsc",
    "start": "ts-node cli/create-task.ts",
    "lint": "eslint .",
    "watch": "tsc -w",
    "format": "prettier --write 'lib/*.{ts,tsx}' 'bin/*.{ts, tsx}' 'test/*.{ts,tsx}'",
    "format:check": "prettier --check 'lib/*.{ts,tsx}'",
    "test": "jest --coverage",
    "test:unit": "jest --coverage --testPathPattern=\\.unit\\.test\\.ts$",
    "test:unit-cdktf": "jest --coverage --testPathPattern=\\.unit\\.test\\.ts$ --setupFilesAfterEnv=./test/setup.js",
    "test:integration": "jest --testPathPattern=\\.int\\.test\\.ts$ --testTimeout=30000",
    "test:integration-cdktf": "jest --testPathPattern=\\.int\\.test\\.ts$ --testTimeout=30000 --setupFilesAfterEnv=./test/setup.js",
    "cdk": "cdk",
    "cdk:synth": "npx cdk synth --context environmentSuffix=${ENVIRONMENT_SUFFIX:-dev}",
    "cdk:bootstrap": "npx cdk bootstrap --context environmentSuffix=${ENVIRONMENT_SUFFIX:-dev}",
    "cdk:deploy": "npx cdk deploy --all --require-approval never --context environmentSuffix=${ENVIRONMENT_SUFFIX:-dev}",
    "cdk:destroy": "npx cdk destroy --all --force --context environmentSuffix=${ENVIRONMENT_SUFFIX:-dev}",
    "cdktf:get": "cdktf get",
    "cdktf:synth": "cdktf synth",
    "cdktf:deploy": "cdktf deploy --auto-approve",
    "cdktf:destroy": "cdktf destroy --auto-approve",
    "cfn:deploy-yaml": "aws cloudformation deploy --template-file lib/TapStack.yml --stack-name TapStack${ENVIRONMENT_SUFFIX:-dev} --capabilities CAPABILITY_IAM --parameter-overrides EnvironmentSuffix=${ENVIRONMENT_SUFFIX:-dev} --tags Repository=${REPOSITORY:-unknown} CommitAuthor=${COMMIT_AUTHOR:-unknown} --s3-bucket=iac-rlhf-cfn-states-${AWS_REGION:-us-east-1} --s3-prefix=${ENVIRONMENT_SUFFIX:-dev}",
    "cfn:deploy-json": "aws cloudformation deploy --template-file lib/TapStack.json --stack-name TapStack${ENVIRONMENT_SUFFIX:-dev} --capabilities CAPABILITY_IAM --parameter-overrides EnvironmentSuffix=${ENVIRONMENT_SUFFIX:-dev} --tags Repository=${REPOSITORY:-unknown} CommitAuthor=${COMMIT_AUTHOR:-unknown} --s3-bucket=iac-rlhf-cfn-states-${AWS_REGION:-us-east-1} --s3-prefix=${ENVIRONMENT_SUFFIX:-dev}",
    "cfn:destroy": "aws cloudformation delete-stack --stack-name TapStack${ENVIRONMENT_SUFFIX:-dev}",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@aws-sdk/client-api-gateway": "^3.855.0",
    "@aws-sdk/client-apigatewayv2": "^3.856.0",
    "@aws-sdk/client-application-auto-scaling": "^3.856.0",
    "@aws-sdk/client-auto-scaling": "^3.846.0",
    "@aws-sdk/client-cloudformation": "^3.848.0",
    "@aws-sdk/client-cloudfront": "^3.846.0",
    "@aws-sdk/client-cloudwatch": "^3.846.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.848.0",
    "@aws-sdk/client-codebuild": "^3.859.0",
    "@aws-sdk/client-codepipeline": "^3.859.0",
    "@aws-sdk/client-config-service": "^3.848.0",
    "@aws-sdk/client-dynamodb": "^3.855.0",
    "@aws-sdk/client-ec2": "^3.848.0",
    "@aws-sdk/client-ecs": "^3.856.0",
    "@aws-sdk/client-elastic-beanstalk": "^3.848.0",
    "@aws-sdk/client-elasticache": "^3.846.0",
    "@aws-sdk/client-eventbridge": "^3.859.0",
    "@aws-sdk/client-iam": "^3.848.0",
    "@aws-sdk/client-kms": "^3.859.0",
    "@aws-sdk/client-lambda": "^3.855.0",
    "@aws-sdk/client-opensearchserverless": "^3.846.0",
    "@aws-sdk/client-rds": "^3.846.0",
    "@aws-sdk/client-route-53": "^3.856.0",
    "@aws-sdk/client-s3": "^3.848.0",
    "@aws-sdk/client-scheduler": "^3.859.0",
    "@aws-sdk/client-secrets-manager": "^3.859.0",
    "@aws-sdk/client-servicediscovery": "^3.856.0",
    "@aws-sdk/client-sfn": "^3.848.0",
    "@aws-sdk/client-sns": "^3.855.0",
    "@aws-sdk/client-sqs": "^3.848.0",
    "@aws-sdk/client-ssm": "^3.848.0",
    "@aws-sdk/client-wafv2": "^3.848.0",
    "@aws-sdk/credential-provider-node": "^3.846.0",
    "@inquirer/prompts": "^7.8.0",
    "@opensearch-project/opensearch": "^3.5.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/release-notes-generator": "14.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.0.11",
    "@types/node-fetch": "^2.6.4",
    "@types/pg": "^8.15.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "aws-cdk": "2.1020.2",
    "aws4": "^1.13.2",
    "axios": "^1.10.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.1",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.7.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "markdownlint-cli": "^0.45.0",
    "mysql2": "^3.14.2",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.3",
    "prettier": "3.6.2",
    "semantic-release": "24.2.6",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-accessanalyzer": "^3.859.0",
    "@aws-sdk/client-acm": "^3.856.0",
    "@aws-sdk/client-cloudtrail": "^3.859.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.848.0",
    "@aws-sdk/client-sqs": "^3.855.0",
    "@aws-sdk/client-ssm": "^3.848.0",
    "@aws-sdk/client-wafv2": "^3.848.0",
    "@cdktf/provider-archive": "^11.0.0",
    "@cdktf/provider-aws": "21.3.0",
    "aws-cdk-lib": "2.204.0",
    "cdktf": "^0.21.0",
    "cdktf-cli": "0.21.0",
    "constructs": "10.4.2"
  }
}
