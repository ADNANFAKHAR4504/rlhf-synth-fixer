  "bin": {
    "tap": "bin/tap.js"
  },
  "scripts": {
    "check-node": "node -e \"const v=process.version; const req='v22.17.0'; if(v!==req){console.error('❌ Node.js version mismatch! Required:',req,'Current:',v); process.exit(1);} console.log('✅ Node.js version is correct:',v);\"",
    "check-python": "python -c \"import sys; req='3.13'; curr=f'{sys.version_info.major}.{sys.version_info.minor}'; print(f'❌ Python version mismatch! Required: {req}, Current: {curr}') or exit(1) if curr != req else print(f'✅ Python version is correct: {curr}');\"",
    "check-pipenv": "bash -c \"req='2025.0.4'; curr=$(pipenv --version 2>/dev/null | grep -oE '[0-9]+\\.[0-9]+\\.[0-9]+' || echo 'not installed'); if [ \\\"\\$curr\\\" != \\\"\\$req\\\" ]; then echo \\\"❌ Pipenv version mismatch! Required: \\$req, Current: \\$curr\\\"; exit 1; fi; echo \\\"✅ Pipenv version is correct: \\$curr\\\"\"",
    "check-versions": "npm run check-node && npm run check-python && npm run check-pipenv",
    "preinstall": "echo 'Skipping version checks for CI/CD'",
    "build": "tsc --skipLibCheck",
    "build:strict": "tsc",
    "start": "ts-node cli/create-task.ts",
    "lint": "eslint .",
    "synth": "cdk synth --all",
    "format": "prettier --write 'lib/*.{ts,tsx}' 'bin/*.{ts,tsx}' 'test/*.{ts,tsx}' 'cli/*.{ts,tsx}'",
    "format:check": "prettier --check 'lib/*.{ts,tsx}'",
    "format-all": "npm run format && npm run lint",
    "watch": "tsc -w",
    "test": "jest --coverage",
    "test:unit": "jest --coverage --testPathPattern=\\.unit\\.test\\.ts$",
    "test:unit-js": "jest --coverage --testPathPattern=\\.unit\\.test\\.mjs$",
    "test:unit-cdktf": "jest --coverage --testPathPattern=\\.unit\\.test\\.ts$",
    "test:integration": "jest --testPathPattern=\\.int\\.test\\.ts$ --testTimeout=30000",
    "test:integration-js": "jest --testPathPattern=\\.int\\.test\\.mjs$ --testTimeout=30000",
    "test:integration-cdktf": "jest --testPathPattern=\\.int\\.test\\.ts$ --testTimeout=30000",
    "cdk": "cdk",
    "cdk:synth": "npx cdk synth --context environmentSuffix=${ENVIRONMENT_SUFFIX:-dev}",
    "cdk:bootstrap": "npx cdk bootstrap --context environmentSuffix=${ENVIRONMENT_SUFFIX:-dev}",
    "cdk:deploy": "npx cdk deploy --all --require-approval never --context environmentSuffix=${ENVIRONMENT_SUFFIX:-dev}",
    "cdk:destroy": "npx cdk destroy --all --force --context environmentSuffix=${ENVIRONMENT_SUFFIX:-dev}",
    "deploy:staging": "cdk deploy --context environment=staging --context owner=cloud-team",
    "deploy:production": "cdk deploy --context environment=production --context owner=cloud-team",
    "destroy:staging": "cdk destroy --context environment=staging",
    "destroy:production": "cdk destroy --context environment=production",
    "cdktf:get": "cdktf get",
    "cdktf:synth": "cdktf synth",
    "cdktf:deploy": "cdktf deploy '*' --auto-approve",
    "cdktf:destroy": "cdktf destroy --auto-approve",
    "tf": "terraform",
    "tf:init": "terraform -chdir=lib init -reconfigure -lock=false -upgrade $TF_INIT_OPTS",
    "tf:plan": "cd lib && terraform plan -lock=false -out=tfplan -input=false",
    "tf:deploy": "cd lib && terraform apply -auto-approve -lock=true -lock-timeout=300s -input=false -var=\\\"created_at=$(date -u +%Y-%m-%dT%H:%M:%SZ)\\\" tfplan",
    "tf:destroy": "cd lib && terraform destroy -auto-approve",
    "tf:fmt": "cd lib && terraform fmt -check -recursive",
    "tf:refresh": "cd lib && terraform apply -refresh-only",
    "tf:state-list": "cd lib && terraform state list",
    "tf:state-show": "cd lib && terraform state show",
    "tf:force-unlock": "cd lib && terraform force-unlock -force",
    "tf:validate": "cd lib && terraform validate",
    "tf:help": "cd lib && terraform -help",
    "tf:output": "cd lib && terraform show -json > ../tf-outputs/tfplan.json",
    "cfn:deploy-yaml": "aws cloudformation deploy --template-file lib/TapStack.yml --stack-name TapStack${ENVIRONMENT_SUFFIX:-dev} --capabilities CAPABILITY_IAM CAPABILITY_NAMED_IAM --parameter-overrides EnvironmentSuffix=${ENVIRONMENT_SUFFIX:-dev} --tags Repository=${REPOSITORY:-unknown} Author=${COMMIT_AUTHOR:-unknown} PRNumber=${PR_NUMBER:-unknown} Team=${TEAM:-unknown} CreatedAt=$(date -u +\"%Y-%m-%dT%H-%M-%SZ\") --s3-bucket=${CFN_S3_BUCKET:-iac-rlhf-cfn-states-${AWS_REGION:-us-east-1}}-${CURRENT_ACCOUNT_ID:-342597974367} --s3-prefix=${ENVIRONMENT_SUFFIX:-dev}",
    "cfn:package-json": "aws cloudformation package --template-file lib/TapStack.json --s3-bucket ${CFN_S3_BUCKET:-iac-rlhf-cfn-states-us-east-1-342597974367} --s3-prefix ${ENVIRONMENT_SUFFIX:-dev} --output-template-file packaged-template.json",
    "cfn:deploy-json": "bash -c 'STACK_NAME=TapStack${ENVIRONMENT_SUFFIX:-dev}; STACK_STATUS=$(aws cloudformation describe-stacks --stack-name \"$STACK_NAME\" --query \"Stacks[0].StackStatus\" --output text 2>/dev/null || echo \"DOES_NOT_EXIST\"); if [[ \"$STACK_STATUS\" == \"ROLLBACK_FAILED\" ]]; then echo \"⚠️ Stack in ROLLBACK_FAILED state, deleting...\"; aws cloudformation delete-stack --stack-name \"$STACK_NAME\"; aws cloudformation wait stack-delete-complete --stack-name \"$STACK_NAME\" || true; fi; npm run cfn:package-json && aws cloudformation deploy --template-file packaged-template.json --stack-name TapStack${ENVIRONMENT_SUFFIX:-dev} --capabilities CAPABILITY_IAM CAPABILITY_NAMED_IAM --parameter-overrides EnvironmentSuffix=${ENVIRONMENT_SUFFIX:-dev} --tags Repository=${REPOSITORY:-unknown} Author=${COMMIT_AUTHOR:-unknown} PRNumber=${PR_NUMBER:-unknown} Team=${TEAM:-unknown} CreatedAt=$(date -u +\"%Y-%m-%dT%H-%M-%SZ\")'",
    "cfn:destroy": "aws cloudformation delete-stack --stack-name TapStack${ENVIRONMENT_SUFFIX:-dev}",
    "pulumi:up": "pulumi up --cwd lib --stack ${ENVIRONMENT_SUFFIX:-dev} --yes",
    "pulumi:deploy": "pulumi up --yes --stack TapStack${ENVIRONMENT_SUFFIX}",
    "pulumi:destroy": "pulumi destroy --yes --stack TapStack${ENVIRONMENT_SUFFIX}",
    "pulumi:delete": "pulumi stack rm --cwd lib --yes",
    "localstack:start": "./scripts/localstack-start.sh",
    "localstack:stop": "./scripts/localstack-stop.sh",
    "localstack:restart": "./scripts/localstack-restart.sh",
    "localstack:clean": "./scripts/localstack-clean.sh",
    "localstack:orchestrate": "./scripts/localstack-orchestrate.sh",
    "localstack:deploy": "./scripts/localstack-deploy.sh",
    "localstack:cleanup": "./scripts/localstack-cleanup.sh",
    "localstack:plan": "./scripts/localstack-plan.sh",
    "localstack:test": "./scripts/localstack-test.sh",
    "localstack:cfn:plan": "./scripts/localstack-cloudformation-plan.sh",
    "localstack:cfn:deploy": "./scripts/localstack-cloudformation-deploy.sh",
    "localstack:cfn:test": "./scripts/localstack-cloudformation-test.sh",
    "localstack:cfn:cleanup": "./scripts/localstack-cloudformation-cleanup.sh",
    "localstack:tf:plan": "./scripts/localstack-terraform-plan.sh",
    "localstack:tf:deploy": "./scripts/localstack-terraform-deploy.sh",
    "localstack:tf:test": "./scripts/localstack-terraform-test.sh",
    "localstack:tf:cleanup": "./scripts/localstack-terraform-cleanup.sh",
    "localstack:cdk:plan": "./scripts/localstack-cdk-plan.sh",
    "localstack:cdk:deploy": "./scripts/localstack-cdk-deploy.sh",
    "localstack:cdk:test": "./scripts/localstack-cdk-test.sh",
    "localstack:cdk:cleanup": "./scripts/localstack-cdk-cleanup.sh",
    "localstack:pulumi:plan": "./scripts/localstack-pulumi-plan.sh",
    "localstack:pulumi:deploy": "./scripts/localstack-pulumi-deploy.sh",
    "localstack:pulumi:test": "./scripts/localstack-pulumi-test.sh",
    "localstack:pulumi:cleanup": "./scripts/localstack-pulumi-cleanup.sh",
    "release": "semantic-release",
    "prepare": "husky"
